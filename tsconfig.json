{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components": ["src/components/*"],
      "@/icons": ["src/assets/icons/*"],
      "@/pages": ["src/pages*"],
      "@/store": ["src/store*"],
      "@/layouts": ["src/layouts*"],
      "@/styles": ["src/styles/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
